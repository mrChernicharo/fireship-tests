{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest --watchAll --verbose"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.3",
    "vite": "^2.3.5"
  },
  "dependencies": {
    "jest": "^27.0.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
